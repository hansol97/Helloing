<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>

    <settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
    
    
<!-- VO 클래스 생성 후 주석 제거
    <typeAliases>
        <typeAlias type="com.jl.helloing." alias="" />
    </typeAliases>
-->
	<typeAliases>
    	<typeAlias type="com.jl.helloing.member.model.vo.Member" alias="member" />
    	<typeAlias type="com.jl.helloing.admin.model.vo.Chatbot" alias="chatbot" />
    </typeAliases>
<!-- mapper.xml 생성 후 주석 제거
    <mappers>
        <mapper resource="/mappers/-mapper.xml" />
        <mapper resource="/mappers/-mapper.xml" />
        <mapper resource="/mappers/-mapper.xml" />
    </mappers>
-->
	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/admin-mapper.xml" />
	</mappers>
    
</configuration>
