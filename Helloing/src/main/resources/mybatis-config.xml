<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>

    <settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
    
	<typeAliases>
    	<typeAlias type="com.jl.helloing.member.model.vo.Member" 		alias="member" />
    	<typeAlias type="com.jl.helloing.admin.model.vo.Chatbot" 		alias="chatbot" />
    	<typeAlias type="com.jl.helloing.business.model.vo.Business" 	alias="business" />
    	<typeAlias type="com.jl.helloing.member.model.vo.AccommWish" 	alias="AccommWish" />
    	<typeAlias type="com.jl.helloing.product.model.vo.Activity" 	alias="activity" />
    	<typeAlias type="com.jl.helloing.product.model.vo.Ticket" 		alias="ticket" />
    </typeAliases>

	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/admin-mapper.xml" />
		<mapper resource="/mappers/business-mapper.xml" />
		<mapper resource="/mappers/product-mapper.xml" />
	</mappers>
    
</configuration>
